{
  "source": ["./style-dictionary/properties/**/*.json"],
  "platforms": {
    "js": {
      "transforms": ["attribute/cti", "name/cti/camel", "color/hex", "size/px"],
      "buildPath": "./src/style/",
      "files": [
        {
          "destination": "variables.js",
          "format": "javascript/es6"
        }
      ]
    }
  }
}
